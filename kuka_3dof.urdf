<robot name="lbr_iiwa_3dof">
  <!-- Materials -->
  <material name="Grey">
    <color rgba="0.2 0.2 0.2 1.0"/>
  </material>
  <material name="Orange">
    <color rgba="1.0 0.423529411765 0.0392156862745 1.0"/>
  </material>
  <material name="Blue">
    <color rgba="0.5 0.7 1.0 1.0"/>
  </material>

  <!-- Base link (link_0) -->
  <link name="lbr_iiwa_link_0">
    <inertial>
      <origin rpy="0 0 0" xyz="-0.1 0 0.07"/>
      <mass value="0.0"/>
      <inertia ixx="0.05" ixy="0" ixz="0" iyy="0.06" iyz="0" izz="0.03"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="pybullet_kuka/kuka_iiwa/meshes/link_0.obj"/>
      </geometry>
      <material name="Grey"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="pybullet_kuka/kuka_iiwa/meshes/link_0.stl"/>
      </geometry>
    </collision>
  </link>

  <!-- Joint 1: connects link_0 to link_1 -->
  <joint name="lbr_iiwa_joint_1" type="revolute">
    <parent link="lbr_iiwa_link_0"/>
    <child link="lbr_iiwa_link_1"/>
    <origin rpy="0 0 0" xyz="0 0 0.1575"/>
    <axis xyz="0 0 1"/>
    <limit effort="300" lower="-2.96705972839" upper="2.96705972839" velocity="10"/>
    <dynamics damping="0.5"/>
  </joint>

  <!-- Link 1 -->
  <link name="lbr_iiwa_link_1">
    <inertial>
      <origin rpy="0 0 0" xyz="0 -0.03 0.12"/>
      <mass value="4"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.09" iyz="0" izz="0.02"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="pybullet_kuka/kuka_iiwa/meshes/link_1.obj"/>
      </geometry>
      <material name="Blue"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="pybullet_kuka/kuka_iiwa/meshes/link_1.stl"/>
      </geometry>
    </collision>
  </link>

  <!-- Joint 2: connects link_1 to link_2 -->
  <joint name="lbr_iiwa_joint_2" type="revolute">
    <parent link="lbr_iiwa_link_1"/>
    <child link="lbr_iiwa_link_2"/>
    <origin rpy="1.57079632679 0 3.14159265359" xyz="0 0 0.2025"/>
    <axis xyz="0 0 1"/>
    <limit effort="300" lower="-2.09439510239" upper="2.09439510239" velocity="10"/>
    <dynamics damping="0.5"/>
  </joint>

  <!-- Link 2 -->
  <link name="lbr_iiwa_link_2">
    <inertial>
      <origin rpy="0 0 0" xyz="0.0003 0.059 0.042"/>
      <mass value="4"/>
      <inertia ixx="0.05" ixy="0" ixz="0" iyy="0.018" iyz="0" izz="0.044"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="pybullet_kuka/kuka_iiwa/meshes/link_2.obj"/>
      </geometry>
      <material name="Blue"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="pybullet_kuka/kuka_iiwa/meshes/link_2.stl"/>
      </geometry>
    </collision>
  </link>

  <!-- Joint 3: connects link_2 to link_3 -->
  <joint name="lbr_iiwa_joint_3" type="revolute">
    <parent link="lbr_iiwa_link_2"/>
    <child link="lbr_iiwa_link_3"/>
    <origin rpy="1.57079632679 0 3.14159265359" xyz="0 0.2045 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="300" lower="-2.96705972839" upper="2.96705972839" velocity="10"/>
    <dynamics damping="0.5"/>
  </joint>

  <!-- Link 3 -->
  <link name="lbr_iiwa_link_3">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0.03 0.13"/>
      <mass value="3"/>
      <inertia ixx="0.08" ixy="0" ixz="0" iyy="0.075" iyz="0" izz="0.01"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="pybullet_kuka/kuka_iiwa/meshes/link_3.obj"/>
      </geometry>
      <material name="Orange"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="pybullet_kuka/kuka_iiwa/meshes/link_3.stl"/>
      </geometry>
    </collision>
  </link>

  <!-- Prismatic gripper attached to link_3 -->

  <joint name="gripper_base" type="fixed">
    <parent link="lbr_iiwa_link_3"/>
    <child link="gripper_body"/>
    <origin rpy="1.57 0 -1.57" xyz="0 0 0.2155"/>
  </joint>

  <link name="gripper_body">
    <inertial>
      <origin xyz="0 -0.049133 0" rpy="0 0 0"/>
      <mass value="0.988882"/>
      <inertia ixx="0.162992" ixy="0" ixz="0" iyy="0.162992" iyz="0" izz="0.164814"/>
    </inertial>
    <visual>
      <origin xyz="0 -0.049133 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.146 0.0725 0.049521"/>
      </geometry>
      <material name="Grey"/>
    </visual>
    <collision>
      <origin xyz="0 -0.049133 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.146 0.0725 0.049521"/>
      </geometry>
    </collision>
  </link>

  <link name="left_finger">
    <inertial>
      <origin xyz="-0.008 0.0 0" rpy="0 0 0"/>
      <mass value="0.05"/>
      <inertia ixx="0.16" ixy="0" ixz="0" iyy="0.16" iyz="0" izz="0.16"/>
    </inertial>
    <visual>
      <origin xyz="-0.008 0.0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.016 0.075 0.02"/>
      </geometry>
      <material name="Grey"/>
    </visual>
    <collision>
      <origin xyz="-0.008 0.0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.016 0.075 0.02"/>
      </geometry>
    </collision>
  </link>

  <link name="right_finger">
    <inertial>
      <origin xyz="0.008 0.0 0" rpy="0 0 0"/>
      <mass value="0.05"/>
      <inertia ixx="0.16" ixy="0" ixz="0" iyy="0.16" iyz="0" izz="0.16"/>
    </inertial>
    <visual>
      <origin xyz="0.008 0.0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.016 0.075 0.02"/>
      </geometry>
      <material name="Grey"/>
    </visual>
    <collision>
      <origin xyz="0.008 0.0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.016 0.075 0.02"/>
      </geometry>
    </collision>
  </link>

  <joint name="left_finger_joint" type="prismatic">
    <parent link="gripper_body"/>
    <child link="left_finger"/>
    <origin xyz="-0.008 0.025 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.055" upper="0" effort="80"/>
  </joint>

  <joint name="right_finger_joint" type="prismatic">
    <parent link="gripper_body"/>
    <child link="right_finger"/>
    <origin xyz="0.008 0.025 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="0" upper="0.055" effort="80"/>
  </joint>

</robot>
